<?xml version="1.0"?>
<PropertyList>

    <path>gear-control.ac</path>

    <animation>
        <type>select</type>
        <object-name>NoseGear.down</object-name>
        <condition>
            <greater-than>
                <property>gear/gear[0]/position-norm</property>
                <value>0.99</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>LHgear.down</object-name>
        <condition>
            <greater-than>
                <property>gear/gear[1]/position-norm</property>
                <value>0.99</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>RHgear.down</object-name>
        <condition>
            <greater-than>
                <property>gear/gear[2]/position-norm</property>
                <value>0.99</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Gear.lever</object-name>
        <property>controls/gear/gear-down</property>
        <factor>15</factor>
        <center>
            <x-m>-0.051</x-m>
            <y-m>0</y-m>
            <z-m>-0.004</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>1</y>
            <z>0.0</z>
        </axis>
    </animation>

</PropertyList>