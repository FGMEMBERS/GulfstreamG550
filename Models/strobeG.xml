<?xml version="1.0"?>

<PropertyList>

 <path>strobe2.ac</path>

 <animation>
  <type>noshadow</type>
  <object-name>strobe2</object-name>
 </animation>

 <animation>
  <type>select</type>
  <object-name>strobe2</object-name>
  <condition>
            <greater-than>
                <property>systems/electrical/outputs/beacon</property>
                <value>5</value>
            </greater-than>
        </condition>
 </animation>

 <animation>
  <type>material</type>
  <object-name>strobe2</object-name>
  <threshold>0.002</threshold>
 </animation>

 <animation>
  <type>billboard</type>
  <object-name>strobe2</object-name>
 </animation>

 <animation>
  <type>scale</type>
  <object-name>strobe2</object-name>
  <property>/sim/time/sun-angle-rad</property>
  <offset>-1.4</offset>
  <x-min>1</x-min>
  <y-min>1</y-min>
  <z-min>1</z-min>
  <x-factor>12</x-factor>
  <y-factor>12</y-factor>
  <z-factor>12</z-factor>
 </animation>

</PropertyList>
