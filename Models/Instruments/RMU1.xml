<!-- ADF active-->

    <animation>
        <type>select</type>
        <object-name>Adf.1</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>instrumentation/rmu/unit/adf-freq</property>
                    <value>999.99</value>
                </greater-than>
                <greater-than>
                    <property>systems/electrical/outputs/adf</property>
                    <value>5</value>
